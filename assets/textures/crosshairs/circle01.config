# The Asgard game engine - crosshairs-texture - circle #01 - configuration file.
#

#
# asset configuration
#

[asset]
class=2d
type=texture


#
# main texture properties
#

[texture]
name=CrosshairsCircleOne
qualityLevel=highest
renderMode=repeat
hardwareResident=true
loadType=plugin-tga
hasGlProperties=false
isBlended=true
hasColormask=false
hasRgbaChange=false
hasSetRgbaChannel=false


#
# load a texture from a TGA image
#

[texture.plugin.tga]
filename=assets/textures/crosshairs/circle01.tga



#
# texture blending
#

[texture.blend]
source=SourceAlpha
destination=OneMinusSourceAlpha

